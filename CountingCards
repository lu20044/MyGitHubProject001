var count = 0;

function cc(card) {
  // 请把你的代码写在这条注释以下
  switch(card){
      case 2:
      case 3:
      case 4:
      case 5:
      case 6:
      count++;
      break;
      case 10 :
      case 'J':
      case 'Q':
      case 'K':
      case 'A':
      count--;
      
  }
  if(count > 0){
    return count + " Bet";
  }else if(count <= 0){
    return count + " Hold";
  }
  
  // 请把你的代码写在这条注释以上
}

// 你可以在这里添加/删除 cc 方法的调用来测试结果
// 提示: 左边只会显示最后一次执行的返回值
cc(2); cc('J'); cc(9); cc(2); cc(7);
